[Version]
user: Ludovica
name: provaukwac

[Corpus]
corpus: ukwac

[Folders]
rootdir: ..
corpus_folder: %(rootdir)s/data/ukwac/
vocabulary_folder: %(rootdir)s/data/vocabulary/
output_folder: %(rootdir)s/data/edges/


[CorpusPortion]
first_ID: 0
last_ID: 10
delete_downloaded: True
basic_url: http://ltdata1.informatik.uni-hamburg.de/depcc/corpus/parsed/part-m-

[MultiProcessing]
workers_n: 4


[GraphParameters]
sentencefilter_fun: length,6,20
min_frequency: 5
lexical_CPoS: N,V,J,R
head_CPoS: N,V,J
max_wildcards: 1
ignored_deprels: cop,prt,nn,aux,auxpass,dep,compound,flat,list
max_distance: 4

[NodeFrequencies]
min_generic: 5
min_struct: 3

