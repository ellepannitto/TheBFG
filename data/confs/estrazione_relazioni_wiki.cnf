[Version]
user: Ludovica
name: estrazione_wiki

[Corpus]
corpus: wiki

[Folders]
rootdir: ..
corpus_folder: /extra/corpora/corpora-en/Wiki20180203/UDs
vocabulary_folder: %(rootdir)s/data/vocabulary/
output_folder: %(rootdir)s/data/edges/


[MultiProcessing]
workers_n: 2


[GraphParameters]
sentencefilter_fun: length,4,100
min_frequency: 50
lexical_CPoS: N,V,J
head_CPoS: V,N
max_wildcards: 1
ignored_deprels: punct,cop,det,ref,case,dep,mark,aux,auxpass,cc,neg,expl,discourse,root
max_distance: 6

[NodeFrequencies]
min_generic: 1000
min_struct: 100

