[Version]
user: Ludovica
name: provawiki

[Corpus]
corpus: wiki

[Folders]
rootdir: ..
corpus_folder: %(rootdir)s/data/wikipedia/
vocabulary_folder: %(rootdir)s/data/vocabulary/
output_folder: %(rootdir)s/data/edges/


[CorpusPortion]
first_ID: 0
last_ID: 5
delete_downloaded: True
basic_url: http://ltdata1.informatik.uni-hamburg.de/depcc/corpus/parsed/part-m-

[MultiProcessing]
workers_n: 3


[GraphParameters]
sentencefilter_fun: length,2,50
min_frequency: 50
lexical_CPoS: N,V,J
head_CPoS: V,N
max_wildcards: 1
ignored_deprels: cop,prt,nn,aux,auxpass,dep,compound,flat,list
max_distance: 5

[NodeFrequencies]
min_generic: 1000
min_struct: 100

