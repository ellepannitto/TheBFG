[Version]
user: Ludovica
name: prova0-100

[Corpus]
corpus: depcc

[Folders]
rootdir: ..
corpus_folder: %(rootdir)s/data/depcc/
vocabulary_folder: %(rootdir)s/data/vocabulary/
output_folder: %(rootdir)s/data/edges/


[CorpusPortion]
first_ID: 0
last_ID: 200
delete_downloaded: True
basic_url: http://ltdata1.informatik.uni-hamburg.de/depcc/corpus/parsed/part-m-

[MultiProcessing]
workers_n: 8


[GraphParameters]
sentencefilter_fun: length,6,20
min_frequency: 1000
lexical_CPoS: N,V,J
head_CPoS: N,V
max_wildcards: 1
ignored_deprels: cop,prt,nn,aux,auxpass,dep,compound,flat,list
max_distance: 5

[NodeFrequencies]
min_generic: 5
min_struct: 3

